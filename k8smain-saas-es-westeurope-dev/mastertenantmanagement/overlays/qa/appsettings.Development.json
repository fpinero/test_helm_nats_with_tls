{
  "_commented_Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o}|{UppercaseLogLevel}|{CorrelationId}|{MachineName}|{Message:lj}{NewLine}"
        }
      },
      {
        "Name": "Debug",
        "Args": {
          "outputTemplate": "{Timestamp:o}|{UppercaseLogLevel}|{CorrelationId}|{MachineName}|{Message:lj}|{Exception}{NewLine}"
        }
      }
    ]
  }
}
