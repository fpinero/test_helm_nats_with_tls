{
  "_comment_Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DBConnection": "User ID =userExample;Password=passwordExample;Server=postgresExample;Port=5432;Database=organizationalstructuredb;Integrated Security=true;Pooling=true;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o}|{UppercaseLogLevel}|{ThreadId}|{CorrelationId}|{MachineName}|{Message:lj}|{FullName}|{MemberName}|{LineNumber}|{Exception}|{Properties:j}{NewLine}"
        }
      }
    ]
  }  
}
